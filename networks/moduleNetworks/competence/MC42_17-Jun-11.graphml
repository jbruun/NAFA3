      <data key="v_id">future</data>
    </node>
    <node id="n3">
      <data key="v_name">present</data>
      <data key="v_id">present</data>
    </node>
    <node id="n4">
      <data key="v_name">simultaneous</data>
      <data key="v_id">simultaneous</data>
    </node>
    <edge source="n1" target="n0">
      <data key="e_weight">1</data>
    </edge>
    <edge source="n2" target="n1">
      <data key="e_weight">1</data>
    </edge>
    <edge source="n3" target="n2">
      <data key="e_weight">2</data>
    </edge>
    <edge source="n4" target="n1">
